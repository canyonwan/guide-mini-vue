#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules:/Users/canyonwan/Documents/github.com/node_modules:/Users/canyonwan/Documents/node_modules:/Users/canyonwan/node_modules:/Users/node_modules:/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/jsdom@16.7.0/node_modules/jsdom/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/jsdom@16.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/acorn@8.7.0/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules:/Users/canyonwan/Documents/github.com/node_modules:/Users/canyonwan/Documents/node_modules:/Users/canyonwan/node_modules:/Users/node_modules:/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/jsdom@16.7.0/node_modules/jsdom/node_modules:/Users/canyonwan/Documents/github.com/guide-mini-vue/node_modules/.pnpm/jsdom@16.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.7.0/node_modules/acorn/bin/acorn" "$@"
fi
